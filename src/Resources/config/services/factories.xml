<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service class="BitBag\InPost\Factory\CreateShippingMethodFactory"
                 id="bitbag.inpost.factory.create_shipping_method_factory"
                 public="true"
        >
            <argument type="service" id="shipping_method.repository" />
            <argument type="service" id="bitbag.inpost.finder.shipping_parameters_finder" />
        </service>

        <service class="BitBag\InPost\Factory\CreateCustomFieldsForPackageDetails"
                 id="bitbag.inpost.factory.create_custom_fields_for_package_details"
                 public="true"
        >
            <argument type="service" id="custom_field_set.repository" />
        </service>
    </services>
</container>
