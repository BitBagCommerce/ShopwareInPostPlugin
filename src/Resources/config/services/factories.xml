<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="BitBag\ShopwareInPostPlugin\Factory\ShippingMethodPayloadFactory"
        >
            <argument type="service" id="shipping_method.repository" />
            <argument type="service" id="BitBag\ShopwareInPostPlugin\Finder\DeliveryTimeFinder" />
            <argument type="service" id="BitBag\ShopwareInPostPlugin\Finder\RuleFinder" />
            <argument type="service" id="BitBag\ShopwareInPostPlugin\Factory\DeliveryTimePayloadFactory" />
            <argument type="service" id="rule.repository" />
            <argument type="service" id="delivery_time.repository" />
        </service>

        <service id="BitBag\ShopwareInPostPlugin\Factory\DeliveryTimePayloadFactory" />

        <service id="BitBag\ShopwareInPostPlugin\Factory\CustomFieldsForPackageDetailsPayloadFactory"
        >
            <argument type="service" id="custom_field_set.repository" />
        </service>

        <service id="BitBag\ShopwareInPostPlugin\Factory\CreateReceiverPackagePayloadFactory" />

        <service id="BitBag\ShopwareInPostPlugin\Factory\CreateParcelPackagePayloadFactory">
            <argument type="service" id="BitBag\ShopwareInPostPlugin\Calculator\OrderWeightCalculator" />
        </service>

        <service id="BitBag\ShopwareInPostPlugin\Factory\CreatePackagePayloadFactory">
            <argument type="service" id="BitBag\ShopwareInPostPlugin\Factory\CreateReceiverPackagePayloadFactory" />
            <argument type="service" id="BitBag\ShopwareInPostPlugin\Factory\CreateParcelPackagePayloadFactory" />
        </service>
    </services>
</container>
